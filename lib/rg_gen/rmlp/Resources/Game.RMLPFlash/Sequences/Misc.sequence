FadeInSequence () 
{
   new PropertyAnimation ()
   {
      new Opacity ()
      {
         setDuration (300);
         setFrame (0.0f);
         setFrame (1.0f);
      }
   }
}

FadeOutSequence () 
{
   new PropertyAnimation ()
   {
      new Opacity ()
      {
         setDuration (300);
         setFrame (1.0f);
         setFrame (0.0f);
      }
   }
}
